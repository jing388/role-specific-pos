            <Button fx:id="confirmButton1" layoutX="246.0" layoutY="356.0" mnemonicParsing="false" onAction="#confirmButton1" text="Confirm" />
            <Label layoutX="94.0" layoutY="252.0" text="Size" textFill="#111315" />
            <ComboBox fx:id="sizeComboBox" layoutX="192.0" layoutY="248.0" prefHeight="26.0" prefWidth="111.0" />
